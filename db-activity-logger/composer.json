{
    "name": "shettyanna/db-activity-logger",
    "description": "Laravel package to log detailed database activity",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shettyanna\\DbActivityLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shettyanna\\DbActivityLogger\\DbActivityLoggerServiceProvider"
            ]
        }
    },
"require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
},
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.23"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
